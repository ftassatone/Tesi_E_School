<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eschool.e_school">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PreLogin"
            android:label="@string/app_name"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HomeClasse"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".HomeDocente"
            android:screenOrientation="landscape">
        </activity>

        <activity android:name=".AggiungiClasse"
            android:screenOrientation="landscape"
            android:parentActivityName=".SezioneScegliClasse">
        </activity>

        <activity android:name=".Registro"
            android:screenOrientation="landscape"
            android:parentActivityName=".SezioneScegliClasse">
        </activity>

        <activity android:name=".SchedaAlunno"
            android:screenOrientation="landscape">
        </activity>

        <activity android:name=".SezioneDiagnostica"
            android:screenOrientation="landscape">
        </activity>

        <activity android:name=".SezioneScegliClasse"
            android:screenOrientation="landscape"
            android:parentActivityName=".HomeDocente">
        </activity>
        
        <activity android:name=".Login"
            android:screenOrientation="landscape"
            android:parentActivityName=".PreLogin">
        </activity>


    </application>


</manifest>